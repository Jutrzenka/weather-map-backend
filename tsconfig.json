{
  "compilerOptions": {
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "strict": true,
    "strictPropertyInitialization": true,
    "sourceMap": true,
    "target": "es3",
    "downlevelIteration": true,
    "lib": ["es6"],
    "outDir": "dist",
    "moduleResolution": "Node",
    "esModuleInterop": true,
  }
}